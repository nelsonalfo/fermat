<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <item>
        <shape
            android:shape="oval"
            >
            <gradient
                android:startColor="#ff297baf"
                android:endColor="#ff16c0e3"
                android:type="linear"
                android:angle="270"
                />
        </shape>
    </item>
    <item>

        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval"
            >

            <stroke
                android:color="#fff"
                android:width="1dp"
                />

            <size android:height="64dp"
                android:width="64dp"/>


            <solid android:color="#fff"/>

        </shape>


    </item>

    <item android:width="62dp"
        android:height="62dp"
        android:gravity="center">

        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval"
            >

            <stroke
                android:color="#666666"
                android:width="2dp"
                />

            <size android:height="22dp"
                android:width="22dp"/>


            <solid android:color="#fff"/>

        </shape>


    </item>

</layer-list>
